<h1>Edit User</h1>

<%= form_for @user do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>
  <br><br>
  
  <%= f.submit "Edit User" %>
<% end %>

<br>

<%= link_to "Home", root_path %>
<%= link_to "Logout", logout_path, method: 'delete' %>